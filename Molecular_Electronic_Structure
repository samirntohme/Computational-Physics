!
}


!-----------------------------------------------------
!              Hartree-Fock Calculations
!-----------------------------------------------------

{rhf;                         
wf,19,1,1;
orbprint,3000;
}

!-----------------------------------------------------
!                       CASSCF 
!-----------------------------------------------------

!thresh,pspace=2;          !Put this statement for Sextet calculations with 7 valence electrons
multi;                       
occ,8,3,3,1;
closed,3,1,1,0;

!Doublet States
           
!wf,19,1,1;state,2;        !1Delta and 1sigma States
!wf,19,2,1;state,2;        !1pi and 1phi States  
!wf,19,3,1;state,2;        !1pi and 1phi States    
!wf,19,4,1;state,1;        !1Delta   

!Quartet States
           
!wf,19,1,3;state,2;        !1Delta and 1sigma States
!wf,19,2,3;state,2;        !1pi and 1phi States  
!wf,19,3,3;state,2;        !1pi and 1phi States   
!wf,19,4,3;state,1;        !1Delta             

!Sextet States
           
wf,19,1,5;state,2;        !1Delta and 1sigma States
wf,19,2,5;state,2;        !1pi and 1phi States  
wf,19,3,5;state,2;        !1pi and 1phi States   
wf,19,4,5;state,1;        !1Delta

!-----------------------------------------------------
!    Specifying internuclear distance points
!-----------------------------------------------------

NatOrb;
Orbital,7100.2;
 
r0 = 1.582
step = 0.02
 
do i=1,1

r = r0 + (i-1)*step
 
dist(i) = r
 
!geomtyp=xyz
geometry={ANGSTROM;
2! number of atoms
GeomXYZ
Ti, 0.0,  0.0,  0.0
N, 0.0,  0.0,  r
}

!-----------------------------------------------------
!                       CASSCF 
!-----------------------------------------------------

!thresh,pspace=2;          !Put this statement for Sextet calculations with 7 valence electrons
multi;                       
occ,8,3,3,1;
closed,3,1,1,0;

!Doublet States
           
!wf,19,1,1;state,2;        !1Delta and 1sigma States
!wf,19,2,1;state,2;        !1pi and 1phi States  
!wf,19,3,1;state,2;        !1pi and 1phi States    
!wf,19,4,1;state,1;        !1Delta   

!Quartet States
           
!wf,19,1,3;state,2;        !1Delta and 1sigma States
!wf,19,2,3;state,2;        !1pi and 1phi States  
!wf,19,3,3;state,2;        !1pi and 1phi States   
!wf,19,4,3;state,1;        !1Delta             

!Sextet States
           
wf,19,1,5;state,2;        !1Delta and 1sigma States
wf,19,2,5;state,2;        !1pi and 1phi States  
wf,19,3,5;state,2;        !1pi and 1phi States   
wf,19,4,5;state,1;        !1Delta

!------------------------------------------------------------------------
!  Calculate expectation values of matrix elements for Lx^2, Ly^2, Lz^2
!------------------------------------------------------------------------
 
TRAN2,LXX,LYY,LZZ;
 
!Doublet states

!D_Lz_1_1(i)= sqrt(abs(LZLZ(1)))
!D_Lz_1_2(i)= sqrt(abs(LZLZ(2)))

!D_Lz_2_1(i)= sqrt(abs(LZLZ(3)))
!D_Lz_2_2(i)= sqrt(abs(LZLZ(4)))

!D_Lz_3_1(i)= sqrt(abs(LZLZ(5)))
!D_Lz_3_2(i)= sqrt(abs(LZLZ(6)))

!D_Lz_4_1(i)= sqrt(abs(LZLZ(7)))

!Quartet states

!Q_Lz_1_1(i)= sqrt(abs(LZLZ(8)))
!Q_Lz_1_2(i)= sqrt(abs(LZLZ(9)))

!Q_Lz_2_1(i)= sqrt(abs(LZLZ(10)))
!Q_Lz_2_2(i)= sqrt(abs(LZLZ(11)))

!Q_Lz_3_1(i)= sqrt(abs(LZLZ(12)))
!Q_Lz_3_2(i)= sqrt(abs(LZLZ(13)))

!Q_Lz_4_1(i)= sqrt(abs(LZLZ(14)))

!Sextet states

S_Lz_1_1(i)= sqrt(abs(LZLZ(15)))
S_Lz_1_2(i)= sqrt(abs(LZLZ(16)))

S_Lz_2_1(i)= sqrt(abs(LZLZ(17)))
S_Lz_2_2(i)= sqrt(abs(LZLZ(18)))

S_Lz_3_1(i)= sqrt(abs(LZLZ(19)))
S_Lz_3_2(i)= sqrt(abs(LZLZ(20)))

S_Lz_4_1(i)= sqrt(abs(LZLZ(21)))
 
!-----------------------------------------------------
!                    CI calculations
!-----------------------------------------------------
 
!*************  CI - Doublet  *************

ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,1;state,2;

D_e_sym1_1(i)= energd(1)
D_e_sym1_2(i)= energd(2)
 
D_e_SO_sym1_1=D_e_sym1_1(i)
D_e_SO_sym1_2=D_e_sym1_2(i)

D_d_sym1_1(i)= DMZ(1)
D_d_sym1_2(i)= DMZ(2)

table,dist,D_Lz_1_1,D_e_sym1_1,D_d_sym1_1
DIGITS,,8
table,dist,D_Lz_1_2,D_e_sym1_2,D_d_sym1_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,2,1;state,2;

D_e_sym2_1(i)= energd(1)
D_e_sym2_2(i)= energd(2)
 
D_e_SO_sym2_1=D_e_sym2_1(i)
D_e_SO_sym2_2=D_e_sym2_2(i)

D_d_sym2_1(i)= DMZ(1)
D_d_sym2_2(i)= DMZ(2)

table,dist,D_Lz_2_1,D_e_sym2_1,D_d_sym2_1
DIGITS,,8
table,dist,D_Lz_2_2,D_e_sym2_2,D_d_sym2_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,3,1;state,2;

D_e_sym3_1(i)= energd(1)
D_e_sym3_2(i)= energd(2)
 
D_e_SO_sym3_1=D_e_sym3_1(i)
D_e_SO_sym3_2=D_e_sym3_2(i)

S_d_sym3_1(i)= DMZ(1)
S_d_sym3_2(i)= DMZ(2)

table,dist,D_Lz_3_1,D_e_sym3_1,D_d_sym3_1
DIGITS,,8
table,dist,D_Lz_3_2,D_e_sym3_2,D_d_sym3_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,1;state,1;

D_e_sym4_1(i)= energd(1)
 
ES_D2=D_e_sym4_1(i)

D_e_SO_sym4_1(i)= DMZ(1)

table,dist,D_Lz_4_1,D_e_sym4_1,D_d_sym4_1
DIGITS,,8


!*************  CI - Quartet  *************

ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,3;state,2;

Q_e_sym1_1(i)= energd(1)
Q_e_sym1_2(i)= energd(2)
 
Q_e_SO_sym1_1=Q_e_sym1_1(i)
Q_e_SO_sym1_2=Q_e_sym1_2(i)

Q_d_sym1_1(i)= DMZ(1)
Q_d_sym1_2(i)= DMZ(2)

table,dist,Q_Lz_1_1,Q_e_sym1_1,Q_d_sym1_1
DIGITS,,8
table,dist,Q_Lz_1_2,Q_e_sym1_2,Q_d_sym1_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,2,3;state,2;

Q_e_sym2_1(i)= energd(1)
Q_e_sym2_2(i)= energd(2)

Q_e_SO_sym2_1=Q_e_sym2_1(i)
Q_e_SO_sym2_2=Q_e_sym2_2(i)

Q_d_sym2_1(i)= DMZ(1)
Q_d_sym2_2(i)= DMZ(2)

table,dist,Q_Lz_2_1,Q_e_sym2_1,Q_d_sym2_1
DIGITS,,8
table,dist,Q_Lz_2_2,Q_e_sym2_2,Q_d_sym2_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
