! ==============================================================================================================
! Program Title: TiN Electronic Structure Calculations
! Author: Dr. Samir Tohme
! Last Modified: February 23, 2023
! Description: This program performs electronic structure calculations for Titanium Nitride (TiN) using MOLPRO. 
! It defines the geometry, basis set, and effective core potentials (ECP) for Titanium (Ti) and Nitrogen (N), 
! and sets up Hartree-Fock and CASSCF calculations for obtaining electronic wavefunctions and energies.
! ==============================================================================================================
memory,1000000000;
Gprint,basis;     !Print basis information
Gprint,orbital;   !Print orbitals in SCF and MCSCF

!geometry=xyz
geometry={ANGSTROM;
2! number of atoms
GeomXYZ
Ti, 0.0, 0.0, 0.0
N, 0.0, 0.0, 1.582
}


basis={

! titanium             (12s,9p,4d) -> [5s,4p,3d]
s, TI , 2335.020000000, 353.044100000, 99.573870000, 77.058450000, 21.546710000, 6.413965000, 6.238279000, 1.996108000, 0.646490000, 0.573285000, 0.073119000, 0.026538000
c, 1.4, 0.06421700, 0.37841200, 0.00000000, 0.67968100
c, 3.6, -0.10947200, 0.00000000, 0.10194300, 0.95462200
c, 7.9, -0.28613700, 0.32182800, 0.85955100
c, 10.11, -0.24245000, 1.10007500
c, 12.12, 1.00000000
p, TI , 99.573870000, 21.546710000, 6.413965000, 6.238279000, 1.996108000, 0.646490000, 0.573285000, 0.073119000, 0.026538000
c, 1.3, 0.13729700, 0.54587500, 0.48906800
c, 4.6, 0.01923700, 0.44044200, 0.63562000
c, 7.8, 0.00029200, 0.99990700
c, 9.9, 1.00000000
d, TI , 7.083666000, 1.709634000, 0.414123000, 0.100130000
c, 1.2, 0.26292100, 0.85577200
c, 3.3, 1.00000000
c, 4.4, 1.00000000
!


ecp,Ti,10,4,3;
!Ti ECP ECP10MDF1 : 10 4 3 77
!Q=12.  MEFIT  MCDHF+Breit+shift, from Dolg 9/28/04
1; 2,     1.00000000000000,     0.00000000000000;
2; 2,    12.68545278327510,   130.80332818219401; 2,     5.60443403028738,    20.28841794549380;
