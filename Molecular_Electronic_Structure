  2,     4.20933536504579,     2.08550079252638; 2,     4.87027289311104,     6.50179581409069;
4; 2,    16.69258854674990,    -9.43456596695940; 2,    17.44887629757160,   -15.01358236125450;
  2,     4.87042604412202,     0.06111200391995; 2,     4.88118144645471,     0.05753055973493;
2; 2,     8.70406407124751,    -1.96536087719181; 2,    10.12717654527650,    -3.75480071683613;
4; 2,    12.16470843145440,   -64.34014485183680; 2,    11.83573845353970,    54.94536681653020;
  2,     4.20933536504579,    -4.17100158505275; 2,     4.87027289311104,     6.50179581409069;
4; 2,    16.69258854674990,     9.43456596695940; 2,    17.44887629757160,   -10.00905490750300;
  2,     4.87042604412202,    -0.06111200391995; 2,     4.88118144645471,     0.03835370648996;
2; 2,     8.70406407124751,     1.31024058479454; 2,    10.12717654527650,    -1.87740035841806;



! nitrogen             (15s,13p,12d,10f,7g,5h,2i) -> [15s,13p,12d,10f,7g,5h,2i]
s, N , 1521.1700000, 330.81400000, 139.2730000, 72.692500000, 36.1708000, 18.515300000, 11.6978000, 8.5691000000, 3.8096600000, 2.2483400000, 1.3124100000, 0.75863700000, 0.37887900000, 0.17588800000, 0.0850000
