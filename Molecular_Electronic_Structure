S_d_sym3_1(i)= DMZ(1)
S_d_sym3_2(i)= DMZ(2)

table,dist,S_Lz_3_1,S_e_sym3_1,S_d_sym3_1
DIGITS,,8
table,dist,S_Lz_3_2,S_e_sym3_2,S_d_sym3_2
DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,5;state,1;

S_e_sym4_1(i)= energd(1)

S_e_SO_sym4_1=S_e_sym4_1(i)

S_d_sym4_1(i)= DMZ(1)

table,dist,S_Lz_4_1,S_e_sym4_1,S_d_sym4_1
DIGITS,,8

!-----------------------------------------------------
!            CI - Spin-Orbit  Calculations
!-----------------------------------------------------

! Doublet States	

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,1;state,2;noexc;save,5100.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,2,1;state,2;noexc;save,5200.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,3,1;state,2;noexc;save,5300.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,1;state,1;noexc;save,5400.2;

! Quartet States	

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,3;state,2;noexc;save,6100.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,2,3;state,2;noexc;save,6200.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,3,3;state,2;noexc;save,6300.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,3;state,1;noexc;save,6400.2;

! Sextet States	

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,5;state,2;noexc;save,5100.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,2,5;state,2;noexc;save,5200.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,3,5;state,2;noexc;save,5300.2;

ci,NOCHECK;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,5;state,1;noexc;save,5400.2;

hlsdiag=[S_e_SO_sym1_1,S_e_SO_sym1_2,S_e_SO_sym2_1,S_e_SO_sym2_2,S_e_SO_sym3_1,D_e_SO_sym3_2,S_e_SO_sym4_1]
ci;options,hlstrans=0;print,HLS=1,VLS=1;hlsmat,ecp,5100.2,5200.2,5300.2,5400.2;
 
text, Final Table Results for Spin-Orbit Calculations

eso_1(i) = energy(1)
eso_2(i) = energy(2)
