DIGITS,,8


ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,4,3;state,1;

Q_e_sym4_1(i)= energd(1)

Q_e_SO_sym4_1=Q_e_sym4_1(i)

Q_d_sym4_1(i)= DMZ(1)

table,dist,Q_Lz_4_1,Q_e_sym4_1,Q_d_sym4_1
DIGITS,,8


!*************  CI - Sextet  *************

ci,NOCHECK;
maxiter,100,1000;
occ,8,3,3,1;
closed,3,1,1,0;
core,3,1,1,0;
wf,19,1,5;state,2;

S_e_sym1_1(i)= energd(1)
S_e_sym1_2(i)= energd(2)

S_e_SO_sym1_1=S_e_sym1_1(i)
S_e_SO_sym1_2=S_e_sym1_2(i)

S_d_sym1_1(i)= DMZ(1)
S_d_sym1_2(i)= DMZ(2)

table,dist,S_Lz_1_1,S_e_sym1_1,S_d_sym1_1
DIGITS,,8
